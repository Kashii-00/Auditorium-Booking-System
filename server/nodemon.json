{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "dist",
    "logs",
    "uploads",
    "backups",
    "*.log"
  ],
  "verbose": true,
  "watch": [
    "src/"
  ],
  "ext": "js,json,ts",
  "exec": "babel-node src/server.js",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": "500"
}
